{
    "version": 2,
    "builds": [
      { "src": "api/graphql.js", "use": "@vercel/node" },
      { "src": "package.json", "use": "@vercel/static-build" }
    ],
    "routes": [
      {
        "src": "/api/graphql",
        "dest": "/api/graphql",
        "methods": ["POST", "OPTIONS"]
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  